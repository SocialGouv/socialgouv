{
  "name": "socialgouv",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "SocialGouv",
  "license": "ISC",
  "homepage": "https://github.com/SocialGouv/socialgouv",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "start": "probot run ./dist/index.js"
  },
  "dependencies": {
    "@types/localtunnel": "^2.0.1",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "graphql": "^15.6.0",
    "graphql-request": "^3.5.0",
    "probot": "^11.0.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "localtunnel": "^2.0.2",
    "nock": "^13.0.5",
    "prettier": "^2.4.1",
    "smee-client": "^1.2.2"
  },
  "engines": {
    "node": ">= 14.x"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
